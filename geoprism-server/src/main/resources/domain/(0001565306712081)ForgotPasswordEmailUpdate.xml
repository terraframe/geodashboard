<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <createOrUpdate>
      <object type="com.runwaysdk.localization.LocalizedValueStore" key="forgotpassword.emailSubject">
        <attribute name="storeTag" value="UIText" />
        <attribute name="storeKey" value="forgotpassword.emailSubject" />
        <attributeStruct name="storeValue">
          <attribute name="defaultLocale" value="Account Recovery Request" />
        </attributeStruct>
      </object>
      <object type="com.runwaysdk.localization.LocalizedValueStore" key="forgotpassword.emailBody">
        <attribute name="storeTag" value="UIText" />
        <attribute name="storeKey" value="forgotpassword.emailBody" />
        <attributeStruct name="storeValue">
          <attribute name="defaultLocale" value="Hello,\n\nYou recently notified us that you forgot your password and want to regain access to your account. To do so, click the link below:\n${link}\n\nThe above link will stop working in ${expireTime} hours.\nIf you did not request this, you can safely ignore this email." />
        </attributeStruct>
      </object>
    </createOrUpdate>
    <create></create>
    <update></update>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>