<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <createOrUpdate>
      <object type="com.runwaysdk.localization.LocalizedValueStore" key="user.invite.email.subject">
        <attribute name="storeTag" value="UIText" />
        <attribute name="storeKey" value="user.invite.email.subject" />
        <attributeStruct name="storeValue">
          <attribute name="defaultLocale" value="Geoprism Registry Account Invitation" />
        </attributeStruct>
      </object>
      <object type="com.runwaysdk.localization.LocalizedValueStore" key="user.invite.email.body">
        <attribute name="storeTag" value="UIText" />
        <attribute name="storeKey" value="user.invite.email.body" />
        <attributeStruct name="storeValue">
          <attribute name="defaultLocale" value="Congratulations!\n\nYou have been invited to create an account with the Geoprism Registry. To do so, click the link below:\n${link}\n\nThe above link will stop working in ${expireTime} hours.\nIf you did not request this, you can safely ignore this email." />
        </attributeStruct>
      </object>
    </createOrUpdate>
    <create></create>
    <update></update>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>